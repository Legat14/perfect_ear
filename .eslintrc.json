{
    "parser": "@typescript-eslint/parser",
    "plugins": ["import", "@typescript-eslint"],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "airbnb-typescript/base",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "env": {
        "es2016": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "@typescript-eslint/no-explicit-any": "error",
        "no-debugger": "off",
        "no-console": 0,
        "class-methods-use-this": "off",
        "no-alert": "off"
    }
}
